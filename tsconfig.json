{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["src/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "preact"
	},
	"include": [".astro/types.d.ts", "**/*.ts", "**/*.tsx", "**/*.astro"],
	"exclude": ["dist", "node_modules", "dist", ".vercel"]
}
